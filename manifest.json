{
  "name": "Slant",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Slant Chrome extension.",
  "browser_action": {
    "default_icon": "./resources/forward-slash.png"
  },

  "background": {
  	"scripts": [
      "/vendor/jquery/dist/jquery.js",
      "js/app.js"
    ]
  },
  "permissions": ["tabs"],
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": [
        "/vendor/jquery/dist/jquery.js",
        "/js/app.js"]
    }
  ]
}